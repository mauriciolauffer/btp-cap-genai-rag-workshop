{
  "name": "btp-cap-genai-rag",
  "version": "1.0.0",
  "description": "GenAI Mail Insights",
  "repository": "https://github.com/SAP-samples/btp-cap-genai-rag",
  "license": "Apache License 2.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@cap-js/hana": "^1.6.1",
    "@langchain/community": "^0.3.29",
    "@langchain/core": "^0.3.36",
    "@sap-ai-sdk/ai-api": "^1.7.0",
    "@sap-ai-sdk/langchain": "^1.7.0",
    "@sap-cloud-sdk/connectivity": "^3.26.0",
    "@sap-cloud-sdk/http-client": "^3.26.0",
    "@sap-cloud-sdk/openapi": "^3.26.0",
    "@sap/cds": "^8.7.1",
    "@sap/xsenv": "^5.4.0",
    "axios": "^1.5.0",
    "express": "^4.21.2",
    "langchain": "^0.3.15",
    "typeorm": "^0.3.20",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@cap-js/cds-typer": "^0.32.1",
    "@cap-js/cds-types": "^0.9.0",
    "@cap-js/sqlite": "^1.8.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.17",
    "@types/sap__xsenv": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "start": "cds-serve",
    "--------DEVELOPMENT-------": "",
    "watch": "npx --yes npm-run-all --npm-path npm --parallel cap router ui5",
    "ui5": "npm run start:hybrid --prefix ./app/ui",
    "ui5:init": "npm install --prefix ./app/ui",
    "cap": "cds-tsx watch --profile hybrid",
    "router": "npm run start:local --prefix ./router",
    "--------UI DEPLOYMENT-------": "",
    "build": "cds-ts build --profile production",
    "build:fast": "cds-ts build --production --no-clean",
    "deployer:hybrid": "cds bind --exec --profile ui-deployment npm run deployer:run",
    "deployer:run": "(cd app/html5-deployer && node ../../node_modules/@sap/html5-app-deployer/index.js)"
  },
  "prettier": {
    "arrowParens": "always",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none",
    "printWidth": 120
  },
  "imports": {
    "#cds-models/*": "./srv/@cds-models/*/index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "cds": {
    "requires": {
      "messaging": {
        "kind": "enterprise-messaging"
      }
    }
  }
}
